    "@ng-bootstrap/ng-bootstrap": "~15.1.1",
    "@ng-select/ng-select": "~11.1.1",
    "@swimlane/ngx-charts": "~20.4.1",
    "@swimlane/ngx-datatable": "~20.1.0",
    "angular-count-to": "0.0.3",
    "angular-mydatepicker": "~0.11.5",
    "angular2-text-mask": "~9.0.0",
    "apexcharts": "~3.40.0",
    "bootstrap": "~4.4.1",
    "canvas": "~2.11.2",
