{
  "name": "EdquillAdmin",
  "version": "0.0.7",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:local",
    "start:local": "ng serve --configuration local --host 0.0.0.0 --port 4211",
    "start:dev": "ng serve --configuration development --host 0.0.0.0 --port 4211",
    "start:staging": "ng serve --configuration staging --host 0.0.0.0 --port 4211",
    "start:prod": "ng serve --configuration production --host 0.0.0.0 --port 4211",
    "lint": "ng lint",
    "test": "ng test --code-coverage",
    "build": "npm run build:local",
    "build:local": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration local",
    "build:dev": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration development",
    "build:staging": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration staging",
    "build:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production",
    "pree2e": "webdriver-manager update",
    "e2e": "ng e2e",
    "build-prod-patch": "npm version patch && ng build --configuration production --aot --outputHashing=all && node ./replace.build.js && node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration production --aot --build-optimizer --base-href=./ && node ./versioning.build.js",
    "build-prod-no-patch": "node ./replace.build.js && ng build --configuration production --aot --build-optimizer --base-href=./ && node ./versioning.build.js"
  },
  "private": true,
  "dependencies": {
    "@akveo/ng2-completer": "~9.0.1",
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
